import keras

import tensorflow

import matplotlib

import sklearn

import os

import numpy as np

from pylab import genfromtxt

from sklearn.metrics import confusion_matrix,accuracy_score 

Using TensorFlow backend.

loaddata1=np.loadtxt("class1_train.txt")

loaddata2=np.loadtxt("class2_train.txt")

loaddata3=np.loadtxt("class3_train.txt")

loaddata4=np.loadtxt("class4_train.txt")

load_train=np.concatenate((loaddata1,loaddata2,loaddata3,loaddata4),axis=0)

#print(load_train)

x=[0]*250

y=[1]*250

z=[2]*250

a=[3]*250

b=x+y+z+a

#print(b)

print(load_train.shape)

(1000, 2)

onehot_lable=keras.utils.to_categorical(b, num_classes=4, dtype='float32')

from matplotlib import pyplot;

from pylab import genfromtxt;

mat0 = genfromtxt("class1_train.txt");

mat1 = genfromtxt("class2_train.txt");

mat2 = genfromtxt("class3_train.txt");

mat3 = genfromtxt("class4_train.txt");

pyplot.scatter(mat0[:,0], mat0[:,1], marker = 'o',label = "class1");

pyplot.scatter(mat1[:,0], mat1[:,1], marker = 'x',label = "class2");

pyplot.scatter(mat2[:,0], mat2[:,1], marker = '*',label = "class3");

pyplot.scatter(mat3[:,0], mat3[:,1], marker = 'd',label = "class4");

pyplot.legend();

pyplot.show();

print(load_train.shape,onehot_lable.shape)

(1000, 2) (1000, 4)

from numpy import loadtxt

from keras.models import Sequential

from keras.layers import Dense

model = Sequential()

model.add(Dense(2, input_dim=2, activation='sigmoid'))

model.add(Dense(15, activation='sigmoid'))

model.add(Dense(30, activation='sigmoid'))

model.add(Dense(4, activation='sigmoid'))

print(model.summary())

Model: "sequential_1"
