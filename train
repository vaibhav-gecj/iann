pred=[]

true=[]

#for i in range(len(load_train)):

 #   pred.append(np.argmax(onehot_lable[i]))

for sample, sample_label in zip(load_train, b):

    sample = sample.reshape([1,2])

    sample_pred=model.predict(sample)

    sample_pred_label = np.argmax(sample_pred, axis = 1)

    true.append(sample_label)

    pred.append(sample_pred_label[0])

#     print(sample, sample_label, sample_pred,sample_label)

    

